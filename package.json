{
  "name": "vue-stackedit",
  "version": "1.0.0",
  "description": "An embedded markdown editor based on stackedit.",
  "module": "dist/vueStackedit.umd.js",
  "unpkg": "dist/vueStackedit.umd.min.js",
  "jsdelivr": "dist/vueStackedit.umd.min.js",
  "repository": {
    "url": "https://github.com/loliconer/vue-stackedit.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/loliconer/vue-stackedit/issues"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "serve": "vue-cli-service serve --port 8089",
    "build": "vue-cli-service build --target lib --name vueStackedit",
    "lint": "vue-cli-service lint"
  },
  "author": "loliconer",
  "license": "MIT",
  "dependencies": {
    "vue": "^2.6.12",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.8",
    "@vue/cli-service": "^4.5.8",
    "bezier-easing": "^2.1.0",
    "diff-match-patch": "^1.0.5",
    "js-yaml": "^3.14.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "lovue": "^3.1.3",
    "markdown-it": "^12.0.2",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-pandoc-renderer": "^1.1.3",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "prismjs": "^1.22.0",
    "raw-loader": "^4.0.2",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-undef": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "Chrome >= 70",
    "Firefox >= 65",
    "Safari >= 11"
  ]
}
