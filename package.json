{
  "name": "vue-stackedit",
  "version": "1.0.0",
  "description": "An embedded markdown editor based on stackedit.",
  "author": "loliconer",
  "scripts": {
    "serve": "vue-cli-service serve --port 8089",
    "build": "vue-cli-service build --target lib --name vueStackedit",
    "lint": "vue-cli-service lint"
  },
  "module": "dist/vueStackedit.umd.js",
  "unpkg": "dist/vueStackedit.umd.min.js",
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "vue": "^2.6.13",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "bezier-easing": "^2.1.0",
    "diff-match-patch": "^1.0.5",
    "js-yaml": "^3.14.0",
    "less": "^4.1.1",
    "less-loader": "^9.0.0",
    "lovue": "^3.1.4",
    "markdown-it": "^12.0.6",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-pandoc-renderer": "^1.1.3",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "prismjs": "^1.23.0",
    "raw-loader": "^4.0.2",
    "vue-template-compiler": "^2.6.13"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-undef": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "Chrome >= 70",
    "Firefox >= 65",
    "Safari >= 11"
  ],
  "bugs": {
    "url": "https://github.com/loliconer/vue-stackedit/issues"
  },
  "jsdelivr": "dist/vueStackedit.umd.min.js",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/loliconer/vue-stackedit.git",
    "type": "git"
  }
}
