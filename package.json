{
  "name": "vue-stackedit",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 8089",
    "build": "vue-cli-service build --target lib --name vueStackedit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "bezier-easing": "^2.1.0",
    "diff-match-patch": "^1.0.4",
    "js-yaml": "^3.13.1",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "lovue": "^2.6.2",
    "markdown-it": "^8.4.2",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-pandoc-renderer": "^1.1.3",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "prismjs": "^1.16.0",
    "raw-loader": "^2.0.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-undef": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie > 0",
    "not ie_mob > 0"
  ]
}
